{
    "editor.accessibilitySupport": "off",
    "editor.comments.ignoreEmptyLines": false,
    "editor.dragAndDrop": false,
    "editor.emptySelectionClipboard": false,
    "editor.folding": false,
    "editor.renderControlCharacters": true,
    "editor.tabCompletion": "on",
    "editor.formatOnSave": true,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.suggest.showStatusBar": true,
    "files.maxMemoryForLargeFilesMB": 8192,
    "workbench.enableExperiments": false,
    "workbench.startupEditor": "none",
    "workbench.preferredHighContrastColorTheme": "Dracula",
    "workbench.preferredLightColorTheme": "Dracula",
    "workbench.tips.enabled": false,
    "workbench.editor.limit.value": 25,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "window.autoDetectHighContrast": false,
    "terminal.external.osxExec": "iTerm.app",
    "update.showReleaseNotes": false,
    "git.enableCommitSigning": true,
    "github.gitAuthentication": false,
    "grunt.autoDetect": "off",
    "gulp.autoDetect": "off",
    "jake.autoDetect": "off",
    "explorer.confirmDelete": false,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "json.schemaDownload.enable": false,
    "settingsSync.keybindingsPerPlatform": false,
    "C_Cpp.intelliSenseMemoryLimit": 8192,
    "python.jediMemoryLimit": 8192,
    "python.analysis.memory.keepLibraryAst": true,
    "editor.smoothScrolling": true,
    "editor.scrollBeyondLastLine": false,
    "workbench.list.smoothScrolling": true,
    "terminal.integrated.scrollback": 100000,
    "python.formatting.provider": "black",
    "python.linting.mypyEnabled": true,
    "python.linting.prospectorEnabled": true,
    "python.defaultInterpreterPath": "python3",
    "python.pythonPath": "python3",
    "latex-workshop.view.pdf.viewer": "tab",
    "workbench.editor.untitled.hint": "hidden",
    "python.experiments.enabled": false,
    "python.linting.prospectorArgs": [
        "--strictness=low"
    ],
    "security.workspace.trust.untrustedFiles": "open",
    "svelte.enable-ts-plugin": true,
    "git.confirmSync": false,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "python.showStartPage": false,
    "svelte.plugin.svelte.format.config.svelteStrictMode": true,
    "[svelte]": {
        "editor.defaultFormatter": "svelte.svelte-vscode"
    },
    "docker.showStartPage": false,
    "editor.bracketPairColorization.enabled": true,
    "editor.hover.delay": 150,
    "editor.quickSuggestionsDelay": 5,
    "workbench.hover.delay": 1000,
    "workbench.sash.hoverDelay": 150,
    "workbench.editor.closeOnFileDelete": true,
    "workbench.editor.limit.enabled": true,
    "debug.console.closeOnEnd": true,
    "debug.toolBarLocation": "docked",
    "keyboard.touchbar.enabled": false,
    "crates.errorDecorator": "❗️",
    "rust-analyzer.debug.openDebugPane": true,
    "rust-analyzer.debug.engine": "vadimcn.vscode-lldb",
    "rust-analyzer.lens.methodReferences": true,
    "html.format.templating": true,
    "workbench.colorTheme": "Dracula",
    "workbench.preferredDarkColorTheme": "Dracula",
    "files.associations": {
        "*.hcl": "terragrunt"
    },
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.gpuAcceleration": "on",
    "rust-analyzer.imports.granularity.enforce": true,
    "rust-analyzer.hover.actions.references.enable": true,
    "rust-analyzer.lru.capacity": 1024,
    "svelte.plugin.svelte.note-new-transformation": false,
    "python.analysis.typeCheckingMode": "basic",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "prettier.bracketSameLine": true,
    "prettier.trailingComma": "all",
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    }
}