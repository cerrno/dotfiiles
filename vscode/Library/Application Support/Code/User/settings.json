{
    "editor.accessibilitySupport": "off",
    "editor.comments.ignoreEmptyLines": false,
    "editor.dragAndDrop": false,
    "editor.emptySelectionClipboard": false,
    "editor.folding": false,
    "editor.renderControlCharacters": true,
    "editor.tabCompletion": "on",
    "editor.formatOnSave": true,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.suggest.showStatusBar": true,
    "files.maxMemoryForLargeFilesMB": 8192,
    "workbench.enableExperiments": false,
    "workbench.startupEditor": "none",
    "workbench.preferredHighContrastColorTheme": "Dracula",
    "workbench.preferredLightColorTheme": "Dracula",
    "workbench.tips.enabled": false,
    "workbench.editor.limit.value": 25,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "window.autoDetectHighContrast": false,
    "terminal.external.osxExec": "iTerm.app",
    "update.showReleaseNotes": false,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "git.enableCommitSigning": true,
    "github.gitAuthentication": false,
    "grunt.autoDetect": "off",
    "gulp.autoDetect": "off",
    "jake.autoDetect": "off",
    "explorer.confirmDelete": false,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "json.schemaDownload.enable": false,
    "settingsSync.keybindingsPerPlatform": false,
    "C_Cpp.intelliSenseMemoryLimit": 8192,
    "python.jediMemoryLimit": 8192,
    "python.analysis.memory.keepLibraryAst": true,
    "rust-analyzer.lruCapacity": 1024,
    "search.maintainFileSearchCache": true,
    "editor.smoothScrolling": true,
    "editor.scrollBeyondLastLine": false,
    "workbench.list.smoothScrolling": true,
    "terminal.integrated.scrollback": 10000,
    "python.formatting.provider": "black",
    "python.linting.mypyEnabled": true,
    "python.linting.prospectorEnabled": true,
    "python.defaultInterpreterPath": "~/.pyenv/shims/python3",
    "python.pythonPath": "~/.pyenv/shims/python3",
    "latex-workshop.view.pdf.viewer": "tab",
    "workbench.editor.untitled.hint": "hidden",
    "python.experiments.enabled": false,
    "python.linting.prospectorArgs": [
        "--strictness=low"
    ],
    "security.workspace.trust.untrustedFiles": "open",
    "rust-analyzer.assist.importEnforceGranularity": true,
    "svelte.enable-ts-plugin": true,
    "git.confirmSync": false,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "python.showStartPage": false,
    "svelte.plugin.svelte.format.config.svelteStrictMode": true,
    "docker.showStartPage": false,
    "editor.bracketPairColorization.enabled": true,
    "editor.hover.delay": 150,
    "editor.quickSuggestionsDelay": 5,
    "workbench.hover.delay": 1000,
    "workbench.sash.hoverDelay": 150,
    "workbench.editor.closeOnFileDelete": true,
    "workbench.editor.limit.enabled": true,
    "debug.console.closeOnEnd": true,
    "debug.toolBarLocation": "docked",
    "keyboard.touchbar.enabled": false,
    "crates.errorDecorator": "❗️",
    "rust-analyzer.debug.openDebugPane": true,
    "rust-analyzer.debug.engine": "vadimcn.vscode-lldb",
    "rust-analyzer.hoverActions.references": true,
    "rust-analyzer.lens.methodReferences": true,
    "html.format.wrapLineLength": 0,
    "html.format.templating": true,
    "workbench.colorTheme": "Dracula",
    "workbench.preferredDarkColorTheme": "Dracula",
}